{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "type": "regex",
      "fileMatch": ["\\.yaml$"],
      "matchStrings": [
        "# ?renovate datasource=(?<datasource>.+?) depName=(?<depName>.+)\\n.+: [\"']?(?<currentValue>.+?)[\"']?\\n"
      ]
    }
  ]
}
